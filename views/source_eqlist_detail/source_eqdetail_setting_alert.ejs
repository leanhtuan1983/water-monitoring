<% if(role == "admin" || role == "admin_client"){ %>
<div style="min-height: 500px;">
    <div class="row mb-4">
        <div class="col-6 text-left">
            <label class="labelInforTab"><%= __('Start')%></label>
            <input type="time" class="form-control"placeholder="HH:mm (Giờ:phút)" id="quanity_str_time" autocomplete="off">
        </div>
        <div class="col-6 text-left">
            <label class="labelInforTab"><%= __('End')%></label>
            <input type="time" class="form-control" placeholder="HH:mm (Giờ:phút)" id="quanity_end_time" autocomplete="off">
        </div>
    </div>
    <div class="row mb-4">
        <div class="col-6 text-left">
            <label class="labelInforTab"><%= __('Estimated quanity (m3)')%></label>
