                <div class="form-check form-check-inline">
                    <input class="form-check-input quanity_checkbox mt-1" type="checkbox" value="" id="quanity_1">
                    <label class="form-check-label" for="quanity_1">
                        <%= __('Sunday')%>
                    </label>
                </div>
            </div>
        </div>
</div>
<div class="modal-footer">
    <button class="btn btn-success" onclick="save_setting_alert()"><%= __('Save')%></button>
</div>
