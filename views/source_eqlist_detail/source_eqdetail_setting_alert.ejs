        <div class="col-6 text-left">
            <label class="labelInforTab"><%= __('Reapeat alert (minute)')%></label>
            <input type="text" class="form-control" id="repeat_alert" autocomplete="off">
        </div>
    </div>
        <div class="row text-left mt-3 p-2">
